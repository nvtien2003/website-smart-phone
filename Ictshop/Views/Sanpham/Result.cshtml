@model IEnumerable<Ictshop.Models.Sanpham>

@{
    ViewBag.Title = "Result";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table>
    <thead>
        <tr>
            <th>Tên sản phẩm</th>
            <th>Mô tả</th>
            <th>Giá</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Tensp</td>
                <td>@item.Mota</td>
                <td>@item.Giatien</td>
            </tr>
}
    </tbody>
</table>
